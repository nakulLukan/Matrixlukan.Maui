<?xml version="1.0" encoding="utf-8" ?>
<base:OverlayBaseView
    x:Class="Matrixlukan.Maui.Controls.Sample.Controls.Overlay.FloatingButton"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:base="clr-namespace:Matrixlukan.Maui.Controls.OverlayView.Control;assembly=Matrixlukan.Maui.Controls.OverlayView">
    <Border
        Margin="24"
        BackgroundColor="PaleGreen"
        StrokeShape="RoundRectangle 25">
        <ImageButton
            BackgroundColor="Transparent"
            Clicked="ImageButton_Clicked"
            HeightRequest="50"
            Source="dotnet_bot.png"
            WidthRequest="50" />
    </Border>
</base:OverlayBaseView>